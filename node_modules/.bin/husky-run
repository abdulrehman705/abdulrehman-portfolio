#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules/husky/bin/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules/husky/bin/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/husky@4.3.8/node_modules:/Users/tallysis/Projects/abdulrehman-portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/bin/run.js" "$@"
else
  exec node  "$basedir/../husky/bin/run.js" "$@"
fi
